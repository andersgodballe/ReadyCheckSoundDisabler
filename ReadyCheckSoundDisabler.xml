<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\..\FrameXML\UI.xsd">
    <Script File="Cli.lua"/>
    <Script File="ReadyCheckSoundDisabler.lua"/>
    <Frame name="ReadyCheckSoundDisablerEventHandler">
        <Scripts>
            <OnEvent function="onevent" />
            <OnLoad function="onload"/>
        </Scripts>
    </Frame>
    <Frame name="ConfigFrame" parent="UIParent" inherits="BasicFrameTemplate" hidden="true" enableMouse="true" movable="true">
        <Size x="234" y="300"/>
        <Anchors>
            <Anchor point="CENTER" />
        </Anchors>
        <Scripts>
            <OnLoad function="ConfigFrame_OnLoad" />
            <OnMouseDown>self:StartMoving();</OnMouseDown>
            <OnMouseUp>self:StopMovingOrSizing();</OnMouseUp>
        </Scripts>
        <Frames>
            <CheckButton name="DisableReadyCheckSoundCheckbox" inherits="UICheckButtonTemplate" enableMouse="true" text="Disable ReadyCheck sound?">
                <Size>
                    <AbsDimension x="33" y="31" />
                </Size>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parent">
                        <Offset>
                            <AbsDimension x="20" y="-40" />
                        </Offset>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick function="DisableReadyCheckSoundCheckbox_OnClick" />
                </Scripts>
                <HighlightColor r="0.72" g="0.53" b="0.04" />
            </CheckButton>

        </Frames>
    </Frame>
</Ui>